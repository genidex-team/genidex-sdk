    "@rollup/plugin-typescript": "^12.1.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "esm": "^3.2.25",
    "geni_data": "file:../geni_data",
    "jest": "^29.7.0",
    "live-server": "^1.2.0",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.5",
